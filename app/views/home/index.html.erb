<script>
//모든 체크모양 삭제 버튼
    $('#clear').click(function(){
       $('.effect-oscar-second').toggleClass('effect-oscar effect-oscar-second');
    });
//입력폼 나타나게 하는 버튼, 체크된 것이 없으면 작동하지 않는다.
    $('form').hide();//hide initially
    $("#confirm").click(function(e){
        var e=$('.effect-oscar-second').length;
        if(e==1){
          $('form').toggle('slow');//or just show instead of toggle
          $('html, body').animate({
            'scrollTop' : $("form").position().top-100
          });
        }
      else alert("한 명의 작가를 선택해 주세요.");
    });
    
//클릭하면 체크뜨게 하는 것.
    $('[class^="chkbutton"]').on('click',function(){
       $(this).parent() //not sure if .special.. is parent, select the correct element
        .toggleClass('effect-oscar effect-oscar-second');
    });
    
    //클릭한 그림이 몇번째인지?
    $("#submit").click(function(){
       var e = $('.effect-oscar-second').index();
       alert(e+1);
    });    

</script>